                        $(document).ready(function() {
                            var type_reset = '<%= info_setup["type_reset"]; %>';
                            if( type_reset !== "no_reset" ){
                                var name_collapse = CollapseType_Resets.filter(t => t.value === type_reset)[0]["collapse"];
                                $('#'+ name_collapse).collapse('show');
                            }
                        });

                        var CollapseType_Resets = [{
                            collapse: 'settingDcom',
                            value: 'dcom'
                        },{
                            collapse: 'settingShaDow',
                            value: 'shadow'
                        },{
                            collapse: 'settingXproxy',
                            value: 'xproxy'
                        },{
                            collapse: 'settingXproxyLan',
                            value: 'xproxy_lan'
                        },{
                            collapse: 'setting911',
                            value: '911'
                        },{
                            collapse: 'settingOxylabThue',
                            value: 'oxylab_thue'
                        },{
                            collapse: 'settingOxylab',
                            value: 'oxylab'
                        },{
                            collapse: 'settingPyproxy',
                            value: 'pyproxy'
                        },{
                            collapse: 'settingPIA',
                            value: 'pia'
                        },{
                            collapse: 'settingBrowsec',
                            value: 'browsec'
                        },{
                            collapse: 'settingBrigthdata',
                            value: 'brightdata'
                        }];
                        function closeCollapseType_Reset(name){
                            CollapseType_Resets.map(t => {
                                if( t["collapse"] !== name ){
                                    $('#'+ t["collapse"]).collapse('hide');
                                }else{
                                    $('#'+ t["collapse"]).collapse('show');
                                }
                            });
                        }
