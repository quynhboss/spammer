                        <label class="radio-inline"><input <%= ((info_setup["type_reset"] === "dcom") ? "checked" : "") %> type="radio" name="type_reset" value="dcom" onclick="closeCollapseType_Reset('settingDcom', 'dcom');">Dcom</label>
